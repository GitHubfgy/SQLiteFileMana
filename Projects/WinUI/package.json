{
  "name": "winui",
  "version": "1.0.0",
  "description": "管理硬盘海量零碎文件的一个 Windows 客户端软件。 使用SQLite 的 BLOB 类型 存储 图片、文档、音乐等文件。",
  "main": "main.js",
  "scripts": {
    "dev": "chcp 65001 && npm run start",
    "start": "electron .",
    "build": "electron-packager . WinUI --win --out ../../Release --arch=x64  --overwrite --electron-version 23.2.1"
  },
  "dependencies": {
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6"
  }
}
